                                            <input id="public_input_appointment_date" t-att-min="'%s' %date" type="date" class="form-control s_website_form_input" name="appointment_date" required="required"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <input type="hidden" name="public_user_id" id="public_user_id" t-att-value="user.id"/>
                            <div class="row radio-toolbar" id="public_schedules"/>
                            <div id="div_button_confirm_schedule" class="clearfix col-12">
                                <button type="submit" id="button_confirm_schedule" class="btn btn-primary float-right btn-md">
                                    Confirm
                                </button>
                            </div>
                        </form>
                    </div>
                </section>
            </div>
        </t>
    </template>

    <template id="portal_confirm_public_booking_form" name="Booking Form">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty oe_website_sale">
                <section class="s_website_form pt16 pb16 o_colored_level">
                    <div class="container">
                        <t t-if="booking">
                        <form method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-model_name="mail.mail">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                            <div class="s_website_form_rows row s_col_no_bgcolor">
                                <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="name">
                                            <span class="s_website_form_label_content">Your Name: </span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="string" class="form-control s_website_form_input" readonly="readonly" t-att-value="user_name"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="email">
                                            <span class="s_website_form_label_content">Your Email: </span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm">
