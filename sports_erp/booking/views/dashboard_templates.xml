<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="portal_booking_dashboard" name="Booking">
        <t t-call="portal.portal_layout"><br/>
            <t t-if="partner.create_booking">
                <div class="row">
                    <form action='/create_booking' method="post">
                        <button type="submit" class="btn btn-primary float-right mb32 ">
                            New Booking
                            <i class="fa fa-pencil"/>
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                            <t t-if="coach">
                                <input type="hidden" name="coach_id" t-att-value="coach.id"/>
                            </t>
                            <t t-if="athlete">
                                <input type="hidden" name="athlete_id" t-att-value="athlete.id"/>
                            </t>
                            <t t-if="fan">
                                <input type="hidden" name="fan_id" t-att-value="fan.id"/>
                            </t>
                        </button>
                    </form>
                </div>
            </t>
            <t t-if="not bookings">
                <div class="alert alert-warning mt8" role="alert">
                    No Assigned Booking found!
                </div>
            </t>
            <t t-else="">
                <h3><i>Assigned Bookings</i></h3>
                <div class="row">
                    <t t-if="bookings" t-call="portal.portal_table">
                        <thead>
                            <th>Ref</th>
                            <th>Appointment Type</th>
                            <th>Start from</th>
                            <th>Description</th>
                            <th>Stage</th>
                            <th/>
                        </thead>
                        <tbody>
                            <tr t-foreach="bookings" t-as="booking">
                                <td>
                                    <span t-field="booking.sequence"/>
                                </td>
                                <td>
                                    <span t-esc="booking.appointment_type_id.name"/>
                                </td>
                                <td>
                                    <span t-field="booking.event_start"/>
                                </td>
                                <td>
                                    <span t-field="booking.description"/>
                                </td>
                                <td>
                                    <span class="badge badge-info badge-pill" t-field="booking.stage_id"/>
                                </td>
                                <td class="formio_form_actions">
                                        <t t-if="not booking.event_id">
                                        <form action='/booking' method="post">
                                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                            <input type="hidden" name="booking_id" t-att-value="booking.id"/>
                                            <button type="submit" class="btn btn-sm btn-primary">
                                                Schedule
                                            </button>
                                        </form>
                                    </t>
<!--                                    </t>-->
                                    <t t-else="else">
                                        <t t-if="booking.state == 'booked'">
                                            <form action='/cancel_booking' method="post">
                                                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                                <input type="hidden" name="booking_id" t-att-value="booking.id"/>
                                                <button type="submit" class="btn btn-sm btn-danger">
                                                    Cancel
                                                </button>
                                            </form>
                                        </t>
                                    </t>
                                </td>
                            </tr>
                        </tbody>
                    </t>
                </div><br/><br/>
            </t>
            <div id="related_booking_button">
                <t t-if="my_bookings">
                    <button id="show_related_bookings" class='input-group-append btn btn-sm btn-primary o_sign_resend_sms'>Related Bookings
                        <span class="fa fa-2x fa-long-arrow-down"></span></button>
                    <button id="hide_related_bookings" class='input-group-append btn btn-sm btn-primary o_sign_resend_sms'>Related Bookings
                        <span class="fa fa-2x fa-long-arrow-up"></span></button>
                </t>
            </div><br/><br/>
            <div id="related_bookings" style="display:none">
                <h3><i>Related Bookings</i></h3>
                    <div class="row">
                        <t t-if="my_bookings" t-call="portal.portal_table">
                            <thead>
                                <th>Ref</th>
                                <th>Appointment Type</th>
                                <th>Start from</th>
                                <th>Assigned to</th>
                                <th>Description</th>
                                <th>Stage</th>
                            </thead>
                            <tbody>
                                <tr t-foreach="my_bookings" t-as="booking">
                                    <td>
                                        <span t-field="booking.sequence"/>
                                    </td>
                                    <td>
                                        <span t-esc="booking.appointment_type_id.name"/>
                                    </td>
                                    <td>
                                        <span t-field="booking.event_start"/>
                                    </td>
                                    <td>
                                        <span t-field="booking.assigned_user_id.name"/>
                                    </td>
                                    <td>
                                        <span t-field="booking.description"/>
                                    </td>
                                    <td>
                                        <span class="badge badge-info badge-pill" t-field="booking.stage_id"/>
                                    </td>
                                </tr>
                            </tbody>
                        </t>
                    </div>
            </div>
        </t>
    </template>

    <template id="portal_booking_form" name="Appointment">
            <t t-call="portal.portal_layout"><br/><br/>
                <t t-set="no_breadcrumbs" t-value="1"/><br/><br/>
                <t t-set="additional_title">Booking Details</t>
                <form action="/my/submit_appointment" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-model_name="mail.mail">
                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                <input type="hidden" name="booking_id" t-att-value="booking.id"/>
                <div class="clearfix">
                    <button type="submit" class="btn btn-primary float-right mb32 ">
                        Confirm
                        <span class="fa fa-long-arrow-right" />
                    </button>
                </div>
                    <div id="schedule_form" class="s_website_form_rows row s_col_no_bgcolor">
                    <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                        <div class="row s_col_no_resize s_col_no_bgcolor">
                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="child">
                                <span class="s_website_form_label_content">Ref : </span>
                                <span class="s_website_form_mark"> *</span>
                            </label>
                            <div class="col-sm">
                                <input type="string" name="sequence" class="form-control s_website_form_input" readonly="readonly" t-att-value="booking.sequence"/>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                        <div class="row s_col_no_resize s_col_no_bgcolor">
                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="child">
                                <span class="s_website_form_label_content">Appointment Type : </span>
                                <span class="s_website_form_mark"> *</span>
                            </label>
                            <div id="type" class="col-sm">
                                <select id="type_selected" name="type" t-attf-class="form-control" required="required">
                                    <option value="">Select...</option>
                                    <t t-foreach="types or []" t-as="type">
                                        <option class="state" t-att-value="type.id">
                                            <t t-esc="type.name"/>
                                        </option>
                                    </t>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                        <div class="row s_col_no_resize s_col_no_bgcolor">
                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="dob">
                                <span class="s_website_form_label_content">Duration :</span>
                                <span class="s_website_form_mark"> *</span>
                            </label>
                            <div class="col-sm">
                                <input id="duration" type="string" class="form-control s_website_form_input" name="duration" required="required" readonly="readonly"/>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                        <div class="row s_col_no_resize s_col_no_bgcolor">
                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="dob">
                                <span class="s_website_form_label_content">Venue :</span>
                                <span class="s_website_form_mark"> *</span>
                            </label>
                            <div class="col-sm">
                                <input id="venue" type="string" class="form-control s_website_form_input" name="duration" required="required" readonly="readonly"/>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                        <div class="row s_col_no_resize s_col_no_bgcolor">
                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="dob">
                                <span class="s_website_form_label_content">Coach :</span>
                                <span class="s_website_form_mark"> *</span>
                            </label>
                            <div class="col-sm">
                                <input id="coach" type="string" class="form-control s_website_form_input" name="duration" required="required" readonly="readonly"/>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                        <div class="row s_col_no_resize s_col_no_bgcolor">
                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="dob">
                                <span class="s_website_form_label_content">Appointment Date :</span>
                                <span class="s_website_form_mark"> *</span>
                            </label>
                            <div class="col-sm">
                                <input id="appointment_date" type="date" class="form-control s_website_form_input" name="appointment_date" required="required"/>
                            </div>
                            <input type="hidden" id="coach_id" name="coach_id"/>
                            <input type="hidden" name="booking_id" t-att-value="booking.id"/>
                        </div>
                    </div>
                </div>
                </form>
                <br/>
            </t>
    </template>

    <template id="portal_cancel_form" name="Cancel Appointment">
            <t t-call="portal.portal_layout">
                <t t-set="no_breadcrumbs" t-value="1"/><br/><br/>
                <t t-set="additional_title">Booking Details</t>
                <form action="/my/cancel_appointment" method="post">
                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                <input type="hidden" name="booking_id" t-att-value="booking.id"/>
                <div class="clearfix">
                    <button type="submit" class="btn btn-primary float-right mb32 ">
                        Cancel
                        <span class="fa fa-long-arrow-right" />
                    </button>
                </div>
                <div class="row o_portal_details">
                    <div class="col-lg-8">
                        <div class="row">
                            <div t-attf-class="form-group col-xl-6">
                                <label class="col-form-label" for="name">Ref</label>
                                <input type="text" name="sequence" t-attf-class="form-control" t-att-value="booking.sequence" readonly="readonly"/>
                            </div>
                            <div t-attf-class="form-group col-xl-6">
                                <label class="col-form-label" for="date">Date</label>
                                <input type="date" name="date" t-attf-class="form-control" t-att-value="booking.event_start" readonly="readonly"/>
                            </div>
                            <div t-attf-class="form-group col-xl-6">
                                <label class="col-form-label" for="date">Reason</label>
                                <input type="text" name="reason" t-attf-class="form-control"/>
                            </div>
                        </div>


                    </div>
                </div>
                </form>
            </t>
    </template>

