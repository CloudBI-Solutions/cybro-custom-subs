                                                <option class="state" t-att-value="appointment_type.id">
                                                    <t t-esc="appointment_type.name"/>
                                                </option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="duration">
                                            <span class="s_website_form_label_content">Duration: </span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="string" class="form-control s_website_form_input" readonly="readonly" t-att-value="duration"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="coach">
                                            <span class="s_website_form_label_content">Coach: </span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="string" class="form-control s_website_form_input" name="coach_id" readonly="readonly" t-att-value="appointment_type.coach_id.name"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="venue">
                                            <span class="s_website_form_label_content">Venue: </span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="string" class="form-control s_website_form_input" readonly="readonly" t-att-value="appointment_type.venue_id.name"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 300px" for="slot">
                                            <span class="s_website_form_label_content">Appointment Slot :</span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="string" class="form-control s_website_form_input" name="appointment_slot" readonly="readonly" t-att-value="slot"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        </t>
                    </div>
                </section>
                <t t-if="access_form">
                    <section class="s_website_form pt16 pb16 o_colored_level">
                        <div  class="oe_structure">
                        </div>
                    </section>
                </t>
            </div>
        </t>
    </template>

