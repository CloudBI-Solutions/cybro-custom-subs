<odoo>
    <template id="latest_blog" inherit_id="website.snippets" name="Latest Blogs">
        <xpath expr="//*[@id='snippet_structure']//t[@t-snippet]"
               position="after">
            <t t-snippet="latest_blog_snippet.s_blog_carousel" t-thumbnail="/latest_blog_snippet/static/src/images/icon.png"/>
        </xpath>
    </template>

         <!-- Snippet Carousel Blog -->

    <template id="s_blog_carousel" name="Latest Blog Snippet">
        <section class="s_blog_carousel pt24 pb24">
            <div class="container">
                <div class="dynamic_snippet_template o_not_editable"/>
            </div>
        </section>
    </template>


</odoo>
